package com.fdmGroup.model;

import jakarta.persistence.Entity;

@Entity
public class Department {
	
	private int deptId;
	private String departNme;
	public Department(int deptId, String departNme) {
		super();
		this.deptId = deptId;
		this.departNme = departNme;
	}
	
	
	public Department() {
		
	}

}
