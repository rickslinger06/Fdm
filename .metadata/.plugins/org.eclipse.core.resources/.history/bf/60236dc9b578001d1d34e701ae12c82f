package com.fdmgroup;

public class Company extends BankCustomer{
	
	private String name;
	private long abn;
	public Company(String name, long abn) {
		super();
		this.name = name;
		this.abn = abn;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public long getAbn() {
		return abn;
	}
	public void setAbn(long abn) {
		this.abn = abn;
	}
	
	
	

}
